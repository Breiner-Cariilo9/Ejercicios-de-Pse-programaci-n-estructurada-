Algoritmo ParqueaderoVehiculos
    Definir numeroVehiculos Como Entero
    Definir placa, horaIngreso, resumenVehiculos Como Caracter
    Definir i Como Entero
    
    resumenVehiculos <- ""
    
    Escribir "=== SISTEMA DE REGISTRO DE PARQUEADERO ==="
    Escribir ""
    
    Escribir "¬øCu√°ntos veh√≠culos ingresar√°n al parqueadero?"
    Leer numeroVehiculos
    
    Si numeroVehiculos > 0 Entonces
        Escribir ""
        Escribir "--- REGISTRO DE VEH√çCULOS ---"
        
        Para i <- 1 Hasta numeroVehiculos Con Paso 1 Hacer
            Escribir ""
            Escribir "Veh√≠culo #", i, ":"
            
            Escribir "Ingrese la placa del veh√≠culo:"
            Leer placa
            
            Escribir "Ingrese la hora de ingreso (HH:MM):"
            Leer horaIngreso
            
            resumenVehiculos <- resumenVehiculos + "Veh√≠culo " + ConvertirATexto(i) + ":" + "\n"
            resumenVehiculos <- resumenVehiculos + "  Placa: " + placa + "\n"
            resumenVehiculos <- resumenVehiculos + "  Hora de ingreso: " + horaIngreso + "\n"
            resumenVehiculos <- resumenVehiculos + "  Estado: ACTIVO" + "\n"
            resumenVehiculos <- resumenVehiculos + "\n"
            
            Escribir "Veh√≠culo registrado exitosamente."
        FinPara
        
        Escribir ""
        Escribir "=== RESUMEN DE VEH√çCULOS REGISTRADOS ==="
        Escribir ""
        Escribir "Total de veh√≠culos en el parqueadero: ", numeroVehiculos
        Escribir ""
        Escribir resumenVehiculos
        
        Escribir "=== ESTAD√çSTICAS ==="
        Escribir "Espacios ocupados: ", numeroVehiculos
        Escribir "Espacios disponibles: ", 100 - numeroVehiculos, " (asumiendo capacidad de 100)"
        
        Si numeroVehiculos >= 80 Entonces
            Escribir "üö® ALERTA: Parqueadero casi lleno"
        SiNo
            Si numeroVehiculos >= 50 Entonces
                Escribir "‚ö† AVISO: Parqueadero medio lleno"
            SiNo
                Escribir "‚úÖ Parqueadero con buena disponibilidad"
            FinSi
        FinSi
        
    SiNo
        Escribir "No se registraron veh√≠culos."
    FinSi
    
FinAlgoritmo
